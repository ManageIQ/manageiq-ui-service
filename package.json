    "> 5%"
  ],
  "engines": {
    "node": ">= 6.9.1",
    "npm": " >= 3.10.3"
  },
  "greenkeeper": {
    "ignore": [
      "angular-datatables",
      "chai",
      "chai-as-promised",
      "d3",
      "typescript"
    ]
  },
  "standard": {
    "globals": [
      "flowchart",
      "__",
      "N_",
      "angular"
    ],
    "parser": "babel-eslint",
    "plugins": [
      "angular"
    ],
    "envs": [
      "browser",
      "node",
      "amd",
      "jquery",
      "es6",
      "protractor",
      "jasmine"
    ],
    "ignore": [
      "*.e2e.js",
      "*.spec.js",
      "/reports/"
    ]
  }
